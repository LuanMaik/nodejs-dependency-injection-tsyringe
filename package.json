    "@types/node": "^17.0.17",
    "@types/supertest": "^2.0.11",
    "jest": "^27.5.1",
    "supertest": "^6.2.2",
    "ts-node": "^10.5.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
